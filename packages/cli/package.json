{
  "name": "@libertas/cli",
  "version": "0.0.1",
  "description": "Command-line interface for libertas credentials management",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "libertas": "./dist/bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsc && node build.mjs",
    "dev": "tsc --watch",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "link": "pnpm link --global"
  },
  "keywords": [
    "cli",
    "credentials",
    "credentials-management",
    "commander"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@libertas/core": "workspace:*",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
